<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Talk</title>
    <link rel="stylesheet" href="./style/style.css">
</head>
<body>
  
    <button onclick="createRoom()"> Create room </button>
    <button onclick="joinRoom()"> Join room</button>
    <button onclick="availlableRooms()">Available rooms</button>

    <dv id="availableRoomsList"></dv>
   <script src="/socket.io/socket.io.js"></script>
   <script>
      const socket = io();

      // Listen for available rooms
      function availlableRooms(){
        socket.on('available-rooms', (rooms) => {
         updateAvailableRoomsList(rooms);
         console.log(rooms);
      });
      }

      function createRoom() {
          socket.emit('createRoom', "room1");
      }

      function joinRoom() {
          socket.emit('joinRoom', "room1");
      }

    
   </script>
</body>
</html>
